
#include <iostream>
using namespace std;

class Calculator {
public:
    void add(int a, int b) {
        cout << "Sum (int): " << a + b << endl;
    }
    void add(float a, float b) {
        cout << "Sum (float): " << a + b << endl;
    }
    void add(double a, double b) {
        cout << "Sum (double): " << a + b << endl;
    }
};

int main() {
    Calculator c;
    c.add(5, 6);
    c.add(3.2f, 4.5f);
    c.add(5.555, 4.444);
    return 0;
}
